<%= render "layouts/errors", :object =>@product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <%= f.label :githublink %>
  <%= f.text_field :githublink %>
  <%= f.label :livelink %>
  <%= f.text_field :livelink %>
  <%= f.submit %>
<% end %>
