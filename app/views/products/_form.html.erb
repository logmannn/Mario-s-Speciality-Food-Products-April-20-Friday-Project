<%= render "layouts/errors", :object =>@product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :countryoforigin %>
  <%= f.text_field :countryoforigin %>
  <%= f.label :cost %>
  <%= f.number_field :cost %>
  <%= f.submit %>
<% end %>
